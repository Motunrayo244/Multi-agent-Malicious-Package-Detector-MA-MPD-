# Copy this file to .env and fill in your actual values
# cp env.sample .env

# ============================================
# REQUIRED: AI Model Configuration
# ============================================
# API key for your AI model provider (OpenAI, Anthropic, etc.)
MODEL_API_KEY=your_api_key_here

# ============================================
# OPTIONAL: Additional AI Provider Keys
# ============================================
# Uncomment and set if using specific providers
# OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here
# GOOGLE_API_KEY=your_google_key_here

# ============================================
# OPTIONAL: LangChain/LangSmith Configuration
# ============================================
# LANGCHAIN_API_KEY=your_langchain_key_here
# LANGCHAIN_TRACING_V2=false
# LANGCHAIN_PROJECT=ma-mpd


# ============================================
# Application Settings
# ============================================
PYTHONUNBUFFERED=1
LOG_LEVEL=INFO

